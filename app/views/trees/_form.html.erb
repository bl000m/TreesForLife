<div >
    <%= simple_form_for(tree) do |f| %>
      <div class="form-content">
        <div class="form-input">
          <%= f.input :name,
                      autofocus: true %>
        </div>
        <div class="form-input">
          <%= f.input :address,
                      autofocus: true%>
        </div>
        <div class="form-input">
          <%= f.input :price_per_year,
                      autofocus: true %>
        </div>
        <div class="form-input">
          <%= f.input :description,
                      autofocus: true %>
        </div>
        <div class="form-input">
          <%= f.input :fruits,
                      autofocus: true %>
        </div>

        <div class="form-input">
          <%= f.input :photo, as: :file, label: "featured photo" %>
        </div>
        <div class= "form-button">
          <%= f.button :submit, "Submit", class: "form-button-text"%>
        </div>
      </div>
    <% end %>
  </div>
